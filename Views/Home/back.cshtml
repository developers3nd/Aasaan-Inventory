
@model IEnumerable<online_store.Models.tblproduct>

@{ 
    Layout = null;
}
<!DOCTYPE html>
<html lang="zxx">

<head>
    <title>A & H ZUPER MART</title>
    <!-- Meta tag Keywords -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8" />
    <meta name="keywords" content="A & H ZUPER MART" />
    <script>
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

        function hideURLbar() {
            window.scrollTo(0, 1);
        }
    </script>
    <!-- //Meta tag Keywords -->
    <!-- Custom-Files -->
    <link rel="stylesheet" href="~/FrontTheme/css/bootstrap.css">
    <!-- Bootstrap-Core-CSS -->
    <link rel="stylesheet" href="~/FrontTheme/css/style.css" type="text/css" media="all" />
    <!-- Style-CSS -->
    <!-- font-awesome-icons -->
    <link href="~/FrontTheme/css/font-awesome.css" rel="stylesheet">
    <!-- //font-awesome-icons -->
    <!-- /Fonts -->
    <link href="//fonts.googleapis.com/css?family=Hind:300,400,500,600,700" rel="stylesheet">
    <link href="~/css/StyleSheet1.css">
    <!-- //Fonts -->
    <!-- jQuery -->
    <script src="~/plugins/jquery/jquery.min.js"></script>

</head>

<body>

    <div class="main-sec" style="min-height:8em;">
        <!-- //header -->
        <header class="py-sm-3 pt-3 pb-2" id="home">
            <div class="container">
                <!-- nav -->
                <div class="top-w3pvt d-flex">
                    <div id="logo">
                        <a href="@Url.Action("Index", "Home")">  <img src="~/dist/img/credit/ShopCheck__1_-removebg-preview.png" alt="User Avatar" style="width:150px;" class="img"></a>
                    </div>


                </div>
                <div class="nav-top-wthree">
                    <nav>
                        <label for="drop" class="toggle"><span class="fa fa-bars"></span></label>
                        <input type="checkbox" id="drop">
                        <ul class="menu">
                            <li class="active"><a href="@Url.Action("Index", "Home")">Home</a></li>

                            <li>
                                <a href="@Url.Action("back", "Home")">Products</a>
                            </li>


                            <li><a href="@Url.Action("Login","Accounts")">Login</a></li>
                        </ul>
                    </nav>
                    <!-- //nav -->
                    <div class="search-form ml-auto">
                        <div class="form-w3layouts-grid">
                            <form action="#" method="post" class="newsletter">
                                <input class="search" type="search" id="txtsearch" placeholder="Search here..." required="">
                                <button class="form-control btn" value=""><span class="fa fa-search"></span></button>
                            </form>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
        </header>
    </div>
    <!-- //header -->
    <!--/banner-->
    <br />
    <div class="container">
        <section class="shipping-wthree">
            <div class="shiopping-grids d-lg-flex">
                <div class="col-lg-4 shiopping-w3pvt-gd text-center">
                    <div class="icon-gd">
                        <span class="fa fa-truck" aria-hidden="true"></span>
                    </div>
                    <div class="icon-gd-info">
                        <h3>FREE SHIPPING</h3>
                        <p>On all order over $2000</p>
                    </div>
                </div>
                <div class="col-lg-4 shiopping-w3pvt-gd sec text-center">
                    <div class="icon-gd"><span class="fa fa-bullhorn" aria-hidden="true"></span></div>
                    <div class="icon-gd-info">
                        <h3>FREE RETURN</h3>
                        <p>On 1st exchange in 30 days</p>
                    </div>
                </div>
                <div class="col-lg-4 shiopping-w3pvt-gd text-center">
                    <div class="icon-gd"> <span class="fa fa-gift" aria-hidden="true"></span></div>
                    <div class="icon-gd-info">
                        <h3>MEMBER DISCOUNT</h3>
                        <p>Register &amp; save up to $29%</p>
                    </div>

                </div>
            </div>

        </section>
    </div>


    <div class="container">
        <section class="banner-bottom py-5">
            <div class="container py-5">
                <h3 class="title-wthree mb-lg-5 mb-4 text-center">Shop Now</h3>
                <!--/row-->
                <div class="row shop-wthree-info text-center products">
                    @foreach (var item in Model)
                    {
                        <div class="col-lg-3 shop-info-grid text-center mt-4 inner_products">
                            <div class="product-shoe-info shoe">
                                <div class="men-thumb-item">
                                    <img src="~/Uploads/@item.pimage" class="img-fluid" alt="">

                                </div>
                                <div class="item-info-product">
                                    <h4>
                                        <a href="#">@item.name</a>
                                    </h4>

                                    <div class="product_price">
                                        <div class="grid-price">
                                            <span class="money"><span class="line"></span>RS.@item.price</span>
                                        </div>
                                        <div class="grid-price">
                                            <a href="@Url.Action("Orderbook","Home",new {pid= @item.pid})" class="btn btn-danger">Order Book</a>
                                        </div>
                                    </div>
                                    <ul class="stars">
                                        <li><a href="#"><span class="fa fa-star" aria-hidden="true"></span></a></li>
                                        <li><a href="#"><span class="fa fa-star" aria-hidden="true"></span></a></li>
                                        <li><a href="#"><span class="fa fa-star-half-o" aria-hidden="true"></span></a></li>
                                        <li><a href="#"><span class="fa fa-star-half-o" aria-hidden="true"></span></a></li>
                                        <li><a href="#"><span class="fa fa-star-o" aria-hidden="true"></span></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>

                    }
                </div>


            </div>
        </section>

    </div>

    <section class="banner-bottom py-5">
        <h1 style="text-align:center;">Delivery Modes    </h1>
        <div class="container py-md-3">
            <div class="row grids-wthree-info text-center">
                <div class="col-lg-4 ab-content">
                    <div class="ab-info-con">
                        <img style="width:200px; height:200px;" src="~/FrontTheme/images/fast-delivery (1).png" alt="Card image cap">

                        <h4>Fast &amp; Free Delivery</h4>
                        <p>Lorem ipsum dolor sit,Nulla pellentesque dolor ipsum laoreet eleifend integer,Pellentesque maximus libero.</p>
                    </div>
                </div>
                <div class="col-lg-4 ab-content">
                    <div class="ab-info-con">
                        <img style="width:200px; height:200px;" src="~/FrontTheme/images/secure-payment.png" alt="Card image cap">

                        <h4>Safe &amp; Secure Payments</h4>
                        <p>Lorem ipsum dolor sit,Nulla pellentesque dolor ipsum laoreet eleifend integer,Pellentesque maximus libero.</p>
                    </div>
                </div>
                <div class="col-lg-4 ab-content">
                    <div class="ab-info-con">
                        <img style="width:200px; height:200px;" src="~/FrontTheme/images/salary.png" alt="Card image cap">

                        <h4>100% Money Back Guarantee</h4>
                        <p>Lorem ipsum dolor sit,Nulla pellentesque dolor ipsum laoreet eleifend integer,Pellentesque maximus libero.</p>
                    </div>
                </div>

            </div>
        </div>
    </section>

    

    <div class="cpy-right text-center py-3">
        <p>
            © 2022 A & H ZUPER MART| All rights reserved

        </p>

    </div>

    <script>
        $(document).ready(function () {
            $("#txtsearch").on("keyup", function () {
                var value = $(this).val().toLowerCase();
                $(".products .inner_products").filter(function () {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                });
            });
        });
    </script>






</body>

</html>